     # bit & check
     0	B = 123         # = 0x7b = 0111 1011
     1	B &= 456        # 456 = 0x1c8 = 1 1100 1000
     2	B = B == 72     # 72 = 0x48 = 0100 1000
     3	jt 5 
     4	jmp 1
     
     5	B = 0
     6	D = B | 65536   # 65536 = 0x10000 = 1 0000 0000 0000 0000
     7	B = 6780005     # = 0x677465 = 0110 0111 0111 0100 0110 0101
     8	C = D & 255     # = 0xff = 1111 1111 1111 1111
     9	B += C
    10	B &= 16777215   # = 0xfffffff
    11	B *= 65899      # = 0x1016b
    12	B &= 16777215   # = 0xfffffff
    13	C = 256 > D
    14	jt 16
    15	jmp 17
    16	jmp 28
    17	C = 0
    18	F = C + 1
    19	F *= 256
    20	F = F > D
    21	jt 23
    22	jmp 24
    23	jmp 26
    24	C++
    25	jmp 18
    26	D = C
    27	jmp 8
    28	C = A == B
    29	jt HALT
    30	jmp 6
